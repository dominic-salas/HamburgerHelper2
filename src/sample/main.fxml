<AnchorPane prefHeight="600.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <children>
      <Button layoutX="256.0" layoutY="532.0" mnemonicParsing="false" onAction="#changeScene" text="Launch Game" textAlignment="CENTER" />
      <Rectangle arcHeight="5.0" arcWidth="5.0" height="351.0" layoutX="298.0" layoutY="151.0" stroke="BLACK" strokeType="INSIDE" width="2.0">
         <effect>
            <ColorAdjust />
         </effect>
      </Rectangle>
      <Label layoutX="31.0" layoutY="151.0" text="Your Stats:">
         <font>
            <Font name="Arial Bold" size="21.0" />
         </font></Label>
      <Label fx:id="profileNameDisplay" layoutX="50.0" layoutY="194.0" text="Profile Name: " />
      <Label fx:id="highScoreDisplay" layoutX="50.0" layoutY="227.0" text="Highscore:" />
      <Label fx:id="creditsDisplay" layoutX="50.0" layoutY="263.0" text="Credits:" />
      <Label fx:id="dateCreatedDisplay" layoutX="50.0" layoutY="300.0" text="Created On:" />
      <TextField fx:id="profileInput" layoutX="144.0" layoutY="80.0" promptText="Select Profile" />
      <Button fx:id="searchButton" layoutX="308.0" layoutY="80.0" mnemonicParsing="false" onAction="#setProfile" text="Search/Create" />
   </children>
</AnchorPane>
